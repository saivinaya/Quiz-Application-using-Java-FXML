              <HBox prefHeight="46.0" prefWidth="510.0">
                  <children>
                      <Label prefHeight="46.0" prefWidth="149.0" text="User Name" />
                      <TextField fx:id="UserName" prefHeight="47.0" prefWidth="187.0" />
                  </children>
              </HBox>
              <HBox prefHeight="46.0" prefWidth="510.0">
                  <children>
                      <Label prefHeight="46.0" prefWidth="147.0" text="Login Name" />
                      <TextField fx:id="loginName" prefHeight="47.0" prefWidth="187.0" />
                  </children>
              </HBox>
              <HBox prefHeight="46.0" prefWidth="510.0">
                  <children>
                      <Label prefHeight="46.0" prefWidth="144.0" text="Password" />
