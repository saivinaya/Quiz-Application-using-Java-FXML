<AnchorPane id="AnchorPane" prefHeight="700" prefWidth="700" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quiz.login.view.SignUpController">
    <children>
       <Button layoutX="263.0" layoutY="547.0" mnemonicParsing="false" onAction="#onClickGotoLogin" prefHeight="36.0" prefWidth="67.0" text="Back" />
        <Button layoutX="427.0" layoutY="547.0" mnemonicParsing="false" onAction="#onClickSignUp" prefHeight="35.0" prefWidth="67.0" text="Sign Up" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="182.0" layoutY="64.0" prefHeight="44.0" prefWidth="356.0" text="Sign Up - New User" textAlignment="CENTER" textFill="#4310da" underline="true">
