<AnchorPane id="AnchorPane" prefHeight="850" prefWidth="850" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quiz.login.view.SignUpController">
    <children>
       <Button layoutX="323.0" layoutY="695.0" mnemonicParsing="false" onAction="#onClickGotoLogin" prefHeight="35.0" prefWidth="88.0" text="Back" />
