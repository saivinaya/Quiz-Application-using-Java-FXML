                      <Label prefHeight="35.0" prefWidth="160.0" text="Student/Admin Name" />
                      <TextField fx:id="UserName" prefHeight="47.0" prefWidth="187.0" />
                  </children>
              </HBox>
              <HBox prefHeight="46.0" prefWidth="510.0">
