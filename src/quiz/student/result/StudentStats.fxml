<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="850.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quiz.student.result.StudentStatsController">
   <children>
      <Pane fx:id="Statistics" prefHeight="850.0" prefWidth="1000.0" styleClass="background" stylesheets="@../../login/view/Login.css">
         <children>
            <Label alignment="CENTER" layoutX="384.0" layoutY="160.0" prefHeight="45.0" prefWidth="252.0" text="Student Statistics" textFill="#3c11c9">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="period" layoutX="530.0" layoutY="524.0" onMouseClicked="#onPeriodSelected" prefHeight="27.0" prefWidth="162.0" />
            <Label layoutX="308.0" layoutY="524.0" prefHeight="27.0" prefWidth="202.0" text="Select Period:">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="308.0" layoutY="427.0" prefHeight="27.0" prefWidth="150.0" text="Select Report:">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <ChoiceBox fx:id="report" layoutX="530.0" layoutY="427.0" onMouseClicked="#reportSelect" prefHeight="27.0" prefWidth="162.0" />
            <Button fx:id="viewStats" layoutX="649.0" layoutY="667.0" mnemonicParsing="false" onAction="#viewStats" prefHeight="50.0" prefWidth="120.0" text="View Stats" />
            <Button layoutX="232.0" layoutY="667.0" mnemonicParsing="false" onAction="#goBack" prefHeight="50.0" prefWidth="120.0" text="Back" />
            <Label layoutX="308.0" layoutY="328.0" text="Select Student:">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <ChoiceBox fx:id="studDrop" layoutX="530.0" layoutY="323.0" onMouseClicked="#studentSelect" prefHeight="31.0" prefWidth="162.0" />
            <Button layoutX="860.0" layoutY="32.0" mnemonicParsing="false" onAction="#logout" prefHeight="23.0" prefWidth="120.0" text="Logout" />
         </children>
      </Pane>
   </children>
</AnchorPane>
